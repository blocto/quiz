{
  "parser": "babel-eslint",
  "extends": ["@portto/eslint-config-portto/browser"],
    "plugins": ["react", "jsx-a11y"],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "import/no-named-as-default": "off",
    "react/forbid-prop-types": "off"
  }
}
